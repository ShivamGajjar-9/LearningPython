num= int(input("enter a number to find it's divisors: "))

def noOfDivisors(num):
    ct = 0
    divisors = []
    for x in range (1, (num // 2 + 1)):
        ct += 1
        if num % x == 0:
            divisors.append(x)
    divisors.append(num)
    print(divisors)
    print(ct)
noOfDivisors(num)     
    
